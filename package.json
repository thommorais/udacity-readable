{
  "name": "readable",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.4",
    "react-throttle": "^0.3.0",
    "redux": "^4.0.0"
  },
  "scripts": {
    "start-react": "react-scripts start",
    "start": "concurrently --kill-others \"npm run start-react\" \"npm run watch-css\" \"cd ./server && npm i && node server.js\" ",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-css": "stylus -c src/styles/ --out ./src",
    "watch-css": "npm run build-css && stylus -c -w src/styles/ --out ./src"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "stylus": "^0.54.5"
  }
}
